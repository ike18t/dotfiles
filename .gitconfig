[alias]
  amend = commit --amend --no-edit
  co = checkout
  st = status
[color]
	ui = auto
  diff = always
  pager = true
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[format]
  pretty = format:"%h %C(yellow)%ci%Creset %Cgreen%an%Creset | %s"

[user]
	name = Isaac Datlof
	email = ike18t@gmail.com
[push]
	default = simple
	autoSetupRemote = true
[url "https://"]
	insteadOf = http://
[log]
	follow = true
[init]
	defaultBranch = main
